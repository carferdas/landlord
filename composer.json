{
    "name": "carlosjfernandes/landlord",
    "description": "A multi-tenant Laravel package, based on single database, simple and ease to use",
    "license": "MIT",
    "keywords": [
        "tenant",
        "tenancy",
        "multitenant",
        "multitenancy",
        "landlord"
    ],
    "authors": [
        {
            "name": "Ademir Mazer Jr",
            "email": "ademir.mazer.jr@torzer.com"
        },
        {
            "name": "Caleb Fidecaro",
            "email": "calebfidecaro@gmail.com"
        },
        {
            "name": "Carlos Fernandes",
            "email": "carlos.fernandes.dsilva@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^8.0|^9.0"
    },
    "require-dev": {
        "mockery/mockery": "~0.9|~1.0",
        "phpunit/phpunit": "5.5.*|6.0.*|7.0.*|^8.0|^9.0",
        "laravel/framework": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "CarlosJFernandes\\Landlord\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CarlosJFernandes\\Landlord\\LandlordServiceProvider"
            ],
            "aliases": {
                "Landlord": "CarlosJFernandes\\Landlord\\Facades\\Landlord"
            }
        }
    }
}
